{
    "plugins": [
        "plugins/markdown",
        "node_modules/jsdoc-babel"
    ],
    "source": {
        "include": ["./src/", "./README.md"]
    },
    "opts": {
        "encoding": "utf8",
        "destination": "./docs/",
        "recurse": true
    },
    "markdown": {
        "hardwrap": true
    }
}
