{
  "name": "f4-subject-selection",
  "version": "1.0.0",
  "description": "F4 Subject Selection client",
  "main": "main.js",
  "scripts": {
    "build": "NODE_ENV=production browserify ./src/main.js | uglifyjs -cm > ../public/bundle.js",
    "watch": "watchify ./src/main.js -o ../public/bundle.js -dv",
    "start": "npm watch",
    "lint": "semistandard",
    "doc": "jsdoc -c ./jsdoc.conf.json"
  },
  "semistandard": {
    "parser": "babel-eslint"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "keywords": [
    "leveldb",
    "subject-selection"
  ],
  "author": "CyrusN <cyrusncy@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-eslint": "^4.1.8",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "browserify": "^12.0.2",
    "chinese-date-formatter": "^1.0.7",
    "freezer-js": "^0.9.6",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^3.10.1",
    "moment": "^2.12.0",
    "nodemon": "^1.9.1",
    "react": "^0.14.7",
    "react-dnd": "^2.1.3",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dnd-touch-backend": "^0.2.4",
    "react-dom": "^0.14.7",
    "react-modal": "^0.6.1",
    "semistandard": "^7.0.5"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.3.13",
    "babel-eslint": "^4.1.6",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "jsdoc": "^3.4.0",
    "jsdoc-babel": "^0.1.0",
    "nodemon": "^1.8.1",
    "semistandard": "^7.0.4",
    "uglify-js": "^2.6.2",
    "watchify": "^3.7.0"
  }
}
